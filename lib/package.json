{
  "name": "@cheap-di/lib",
  "version": "0.0.0",
  "scripts": {
    "compile:cjs": "tsc --build ./tsconfig.cjs.json",
    "compile:esm": "tsc --build ./tsconfig.esm.json",
    "compile:types": "tsc --build ./tsconfig.types.json",
    "compile": "tsc --build ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "prepare-package-json": "ts-node ./scripts/prepare-package-json.ts",
    "build:clean": "rimraf -rf ./dist",
    "build": "npm-run-all build:clean compile && yarn prepare-package-json"
  },
  "dependencies": {
    "ts-creator": "^1.2.5",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@types/node": "20.8.7",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.5",
    "ts-node": "10.9.1"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "default": "./dist/esm/*.js"
    },
    "./package.json": "./package.json"
  }
}
