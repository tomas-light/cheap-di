{
  "name": "cheap-di",
  "version": "4.0.0-rc.1",
  "description": "TypeScript dependency injection like Autofac in .Net",
  "workspaces": [
    "lib",
    "tests/*"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "test": "yarn workspace @cheap-di/tests run test"
  },
  "dependencies": {
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "eslint": "8.52.0",
    "prettier": "3.0.3"
  },
  "files": [
    "lib/dist/**/*"
  ],
  "main": "lib/dist/index.js",
  "types": "lib/dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TomasLight/cheap-di.git"
  },
  "author": "TomasLight",
  "license": "MIT",
  "homepage": "https://github.com/TomasLight/cheap-di#readme",
  "keywords": [
    "dependency",
    "injection",
    "dependency injection",
    "di",
    "cheap di",
    "instance type"
  ],
  "packageManager": "yarn@4.0.0"
}
