{
  "name": "cheap-di",
  "version": "4.0.0-rc.1",
  "description": "TypeScript dependency injection like Autofac in .Net",
  "workspaces": [
    "lib",
    "tests/*"
  ],
  "scripts": {
    "build": "yarn workspace @cheap-di/lib run build",
    "test": "yarn workspace @cheap-di/webpack-project run webpack:dev"
  },
  "dependencies": {
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "eslint": "8.52.0",
    "prettier": "3.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TomasLight/cheap-di.git"
  },
  "author": "TomasLight",
  "license": "MIT",
  "homepage": "https://github.com/TomasLight/cheap-di#readme",
  "keywords": [
    "dependency",
    "injection",
    "dependency injection",
    "di",
    "cheap di",
    "instance type"
  ],
  "packageManager": "yarn@4.0.0",
  "main": "./lib/dist/cjs/lib.js",
  "module": "./lib/dist/esm/lib.js",
  "types": "index.d.ts",
  "exports": {
    "./*": {
      "types": "./lib/dist/types/*.d.ts",
      "require": "./lib/dist/cjs/*.js",
      "import": "./lib/dist/esm/*.js",
      "default": "./lib/dist/esm/*.js"
    },
    "./package.json": "./lib/package.json"
  }
}
