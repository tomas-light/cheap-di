{
  "moduleNameMapper": {
    // replaces
    // `import {} from './ContainerImpl.js'` with
    // `import {} from './ContainerImpl'`
    "^(\\.\\.?\\/.+)\\.js?$": "$1"
  },
  "transform": {
    "^.+\\.ts?$": [
      "ts-jest",
      {
        "astTransformers": {
           "before": ["cheap-di-ts-transform"]
//          "before": ["<rootDir>/../../packages/cheap-di-ts-transform/src/index.ts"]
        }
      }
    ]
  }
}
