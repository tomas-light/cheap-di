    "cheap-di": "^4.0.0-rc-30",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "cheap-di": ">= 4.0.0-rc-30",
    "react": ">= 18.2.0",
    "react-dom": ">= 18.2.0"
