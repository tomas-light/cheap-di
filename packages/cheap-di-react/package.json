{
  "name": "@cheap-di-react/src",
  "version": "4.1.0",
  "description": "Dependency injection based on cheap-di for using in React components with React Context",
  "scripts": {
    "compile": "tsc --build tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "prepare-package-json": "ts-node scripts/prepare-package-json.ts",
    "build:clean": "rimraf -rf ./dist",
    "build": "npm-run-all build:clean compile prepare-package-json"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.38",
    "cheap-di": "^4.0.1",
    "react": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "cheap-di": ">= 4.0.1",
    "react": ">= 18.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomas-light/cheap-di.git"
  },
  "author": "TomasLight",
  "license": "MIT",
  "homepage": "https://github.com/tomas-light/cheap-di/tree/master/packages/cheap-di-react#readme",
  "keywords": [
    "dependency",
    "injection",
    "dependency injection",
    "dependency injection react",
    "react",
    "cheap-di"
  ],
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "build-instructions": {
    "files": [
      "cjs/**/*",
      "esm/**/*",
      "types/**/*",
      "package.json",
      "README.md",
      "LICENSE"
    ],
    "main": "./cjs/index.js",
    "module": "./esm/index.js",
    "types": "./types/index.d.ts"
  }
}
