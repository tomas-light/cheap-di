{
  "name": "cheap-di-ts-transform",
  "version": "1.0.0-rc-6",
  "scripts": {
    "check-code-gen": "ts-node src/helpers/testCodeGeneration.ts",
    "compile": "tsc --build tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "prepare-package-json": "ts-node scripts/prepare-package-json.ts",
    "build:clean": "rimraf -rf ./dist",
    "build": "npm-run-all build:clean compile prepare-package-json"
  },
  "dependencies": {
    "cheap-di": "4.0.0-rc-5",
    "ts-creator": "1.2.5",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@types/node": "20.8.10",
    "rimraf": "5.0.5",
    "ts-node": "10.9.1"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts"
}
